/**
 * 8-way 16-bit multiplexor:
 * out = a if sel == 000
 *       b if sel == 001
 *       etc.
 *       h if sel == 111
 */

CHIP Mux8Way16 {
    IN a[16], b[16], c[16], d[16],
       e[16], f[16], g[16], h[16],
       sel[3];
    OUT out[16];

    PARTS:
    // Put your code here:
    // CÃ³digo completado por @.cronin
    Mux16(a=a, b=e, sel=sel[2], out=out01);
    Mux16(a=b, b=f, sel=sel[2], out=out02);
    Mux16(a=c, b=g, sel=sel[2], out=out03);
    Mux16(a=d, b=h, sel=sel[2], out=out04);
    Mux4Way16(a=out01, b=out02, c=out03, d=out04, sel=sel[0..1], out=out);
}
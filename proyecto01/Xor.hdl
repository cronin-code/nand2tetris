/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    // CÃ³digo completado por @.cronin
    Nand(a=a, b=b, out=out01);
    Nand(a=a, b=out01, out=out02);
    Nand(a=out01, b=b, out=out03);
    Nand(a=out02, b=out03, out=out);
}